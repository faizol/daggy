#include <windows.h>
#ifdef APP_ICON
SU_ICON ICON APP_ICON
#endif
#ifndef DEBUG
#define FILE_FLAG                   0
#else
#define FILE_FLAG                   VS_FF_DEBUG
#endif
VS_VERSION_INFO VERSIONINFO
    FILEVERSION @PROJECT_VERSION@
    PRODUCTVERSION  @PROJECT_VERSION@
    FILEFLAGSMASK 0x3fL
    FILEFLAGS FILE_FLAG
    FILEOS VOS__WINDOWS32
    FILETYPE @FILETYPE@
    FILESUBTYPE 0x0L
    BEGIN
        BLOCK "StringFileInfo"
        BEGIN
            BLOCK "040904b0"
            BEGIN
                VALUE "CompanyName",      "@PROJECT_VENDOR@"
                VALUE "FileDescription",  "@PROJECT_DESCRIPTION@"
                VALUE "FileVersion",      "@VERSION_COMMIT@"
                VALUE "LegalCopyright",   "@PROJECT_COPYRIGTH@"
                VALUE "ProductName",      "@TARGET@"
                VALUE "ProductVersion",   "@VERSION@"
            END
        END
        BLOCK "VarFileInfo"
        BEGIN
                VALUE "Translation", 0x0409, 1200
        END
    END
